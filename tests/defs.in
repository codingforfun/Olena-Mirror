abs_top_builddir=@abs_top_builddir@
abs_top_srcdir=@abs_top_builddir@

CXXFLAGS="-Wall -W -ftemplate-depth-50 -pedantic -I$abs_top_builddir -I$abs_top_srcdir"

# Turn on shell traces when VERBOSE=x.
if test "x$VERBOSE" = xx; then
  set -x
else
  :
fi
